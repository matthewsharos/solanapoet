    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/multer": "^1.4.12",
    "@types/next": "^8.0.7",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
