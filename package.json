    "@metaplex-foundation/umi-signer-wallet-adapters": "^0.9.1",
    "@metaplex-foundation/umi-web3js-adapters": "^0.9.1",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@solana/pay": "^0.2.5",
    "@solana/spl-token": "^0.4.0",
    "@solana/wallet-adapter-base": "^0.9.23",
