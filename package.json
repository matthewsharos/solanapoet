    "@solana/web3.js": "^1.98.0",
    "@types/react-toastify": "^4.0.2",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.8.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "google-auth-library": "^9.15.1",
    "googleapis": "^146.0.0",
    "http2-wrapper": "^2.2.1",
    "idb": "^8.0.2",
    "memfs": "^4.17.0",
    "mongoose": "^8.11.0",
    "multer": "^1.4.5-lts.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.30.0",
