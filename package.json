{
  "name": "solanapoet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "tsx src/server/index.ts",
    "dev:server": "nodemon --exec tsx src/server/index.ts",
    "precleanup": "pkill -f 'node.*src/server/index.js' || true",
    "cleanup": "echo 'Cleaned up existing processes'",
    "prestart": "npm run cleanup",
    "start": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "create-auction-house": "tsx src/scripts/create-auction-house.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
