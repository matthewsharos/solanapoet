{
  "name": "solanapoet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:vercel": "npm run build && npm run build:server && cp -r dist dist-copy && mkdir -p .vercel/output/static && cp -r dist-copy/* .vercel/output/static/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "ts-node src/server/index.ts",
    "dev:server": "nodemon --exec tsx src/server/index.ts",
    "precleanup": "pkill -f 'node.*src/server/index.js' || true",
    "cleanup": "echo 'Cleaned up existing processes'",
    "prestart": "npm run cleanup",
    "start": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "create-auction-house": "tsx src/scripts/create-auction-house.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
